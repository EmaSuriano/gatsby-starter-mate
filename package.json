{
  "name": "gatsby-starter-mate",
  "version": "0.0.0",
  "description": "An accessible and fast portfolio starter for Gatsby integrated with Contentful CMS",
  "license": "MIT",
  "scripts": {
    "build": "env-cmd --silent gatsby build",
    "start": "env-cmd --silent gatsby develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "setup": "ts-node ./bin/setup.ts",
    "typecheck": "tsc --noEmit",
    "scan-dead-code": "npx unimported"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EmaSuriano/gatsby-starter-mate.git"
  },
  "keywords": [
    "gatsby",
    "starter",
    "contentful",
    "PWA",
    "portfolio"
  ],
  "author": "Ema Suriano",
  "bugs": {
    "url": "https://github.com/EmaSuriano/gatsby-starter-mate/issues"
  },
  "homepage": "https://github.com/EmaSuriano/gatsby-starter-mate#readme",
  "dependencies": {
    "gatsby-plugin-google-analytics": "^4.25.0",
    "@emotion/react": "^11.11.1",
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-brands-svg-icons": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@rebass/preset": "^4.0.5",
    "@tippy.js/react": "^3.1.1",
    "gatsby": "^4.25.7",
    "babel-plugin-styled-components": "^2.1.4",
    "gatsby-plugin-fontawesome-css": "^1.2.0",
    "gatsby-plugin-image": "^2.10.0",
    "gatsby-plugin-manifest": "^4.10.1",
    "gatsby-plugin-react-helmet": "^5.10.0",
    "gatsby-plugin-sharp": "^4.25.1",
    "gatsby-plugin-styled-components": "^5.10.0",
    "gatsby-plugin-typescript": "^4.10.0",
    "gatsby-source-contentful": "^7.8.1",
    "gatsby-source-medium": "^4.10.0",
    "gatsby-transformer-remark": "^5.25.1",
    "react-awesome-reveal": "^4.2.7",
    "react-headroom": "^3.2.1",
    "react-helmet": "^6.1.0",
    "react-markdown": "^8.0.1",
    "react-text-loop": "^2.3.0",
    "rebass": "^4.0.7",

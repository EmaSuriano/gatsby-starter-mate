{
  "name": "gatsby-starter-mate",
  "version": "2.5.2",
  "description": "Gatsby v2 starter to create a top notch portfolio!",
  "main": "index.js",
  "scripts": {
    "build": "gatsby clean && gatsby build",
    "develop": "gatsby clean && gatsby develop",
    "start": "yarn develop",
    "serve": "yarn build && gatsby serve -p 9000",
    "lint": "eslint ./src",
    "setup": "node ./bin/setup.js",
    "test": "percy exec -t 350 -h cdn-images-1.medium.com -- cypress run",
    "test:open": "percy exec -t 350 -h cdn-images-1.medium.com -- cypress open",
    "test:ci": "DETERMINISTIC=true start-server-and-test serve 9000"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EmaSuriano/gatsby-starter-mate.git"
  },
  "keywords": [
    "gatsby",
    "starter",
    "contentful",
    "PWA",
    "portfolio"
  ],
  "author": "Ema Suriano",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EmaSuriano/gatsby-starter-mate/issues"
  },
  "homepage": "https://github.com/EmaSuriano/gatsby-starter-mate#readme",
  "dependencies": {
    "@rebass/preset": "^4.0.5",
    "@tippy.js/react": "^3.1.1",
    "contentful": "^7.14.2",
    "dotenv": "^8.2.0",
    "emotion-theming": "^10.0.27",
    "envfile": "^5.0.0",
    "gatsby": "2.15.22",
    "gatsby-plugin-google-analytics": "^2.2.2",
    "gatsby-plugin-manifest": "^2.3.3",
    "gatsby-plugin-offline": "^2.0.25",
    "gatsby-plugin-preload-link-crossorigin": "^1.0.2",
    "gatsby-plugin-react-helmet": "^3.2.2",
    "gatsby-plugin-styled-components": "^3.2.1",
    "gatsby-plugin-web-font-loader": "^1.0.4",
    "gatsby-source-contentful": "2.2.7",
    "gatsby-source-medium": "^2.2.1",
    "gatsby-transformer-remark": "^2.7.1",
    "inquirer": "^7.0.4",
    "prop-types": "^15.6.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-fontawesome": "^1.6.1",
    "react-headroom": "^3.0.0",
    "react-helmet": "^5.2.0",
    "react-markdown": "^4.3.1",
    "react-reveal": "^1.2.2",
    "react-scroll-section": "^1.3.0",
    "react-text-loop": "^2.0.1",
    "rebass": "^4.0.7",
    "styled-components": "^5.1.0"
  },
  "devDependencies": {
    "@percy/cypress": "^2.1.1",
    "axe-core": "^3.5.3",
    "chalk": "^4.0.0",
    "contentful-import": "^7.7.7",
    "cypress": "^4.3.0",
    "cypress-axe": "^0.8.1",
    "eslint": "^5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-cypress": "^2.8.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.18.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.1.3",
    "prettier": "^2.0.4",
    "start-server-and-test": "^1.10.8"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write"
    ]
  }
}

{
  "name": "gatsby-starter-mate",
  "version": "2.5.2",
  "description": "Gatsby v2 starter to create a top notch portfolio!",
  "main": "index.js",
  "scripts": {
    "prepare": "node scripts/get-about",
    "build": "yarn prepare && gatsby build",
    "develop": "yarn prepare && gatsby develop",
    "start": "yarn develop",
    "serve": "yarn build && gatsby serve -p 9000",
    "lint": "eslint ./src",
    "setup": "node ./bin/setup.js",
    "test": "percy exec -t 350 -h cdn-images-1.medium.com -- cypress run",
    "test:open": "percy exec -t 350 -h cdn-images-1.medium.com -- cypress open",
    "test:ci": "DETERMINISTIC=true start-server-and-test 8000"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EmaSuriano/gatsby-starter-mate.git"
  },
  "keywords": [
    "gatsby",
    "starter",
    "contentful",
    "PWA",
    "portfolio"
  ],
  "author": "Ema Suriano",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EmaSuriano/gatsby-starter-mate/issues"
  },
  "homepage": "https://github.com/EmaSuriano/gatsby-starter-mate#readme",
  "dependencies": {
    "@babel/preset-env": "^7.10.2",
    "@rebass/preset": "^4.0.5",
    "@tippy.js/react": "^3.1.1",
    "contentful": "^7.14.3",
    "gatsby": "^2.23.3",
    "gatsby-plugin-google-analytics": "^2.3.2",
    "gatsby-plugin-manifest": "^2.4.3",
    "gatsby-plugin-offline": "^3.2.9",
    "gatsby-plugin-react-helmet": "^3.3.1",
    "gatsby-plugin-styled-components": "^3.3.2",
    "gatsby-source-contentful": "^2.3.15",
    "gatsby-source-medium": "^2.3.4",
    "gatsby-transformer-remark": "^2.8.13",
    "prop-types": "^15.6.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-fontawesome": "^1.6.1",
    "react-headroom": "^3.0.0",
    "react-helmet": "^6.0.0",
    "react-markdown": "^4.3.1",
    "react-reveal": "^1.2.2",
    "react-scroll-section": "^2.0.0",
    "react-text-loop": "^2.0.1",
    "rebass": "^4.0.7",
    "styled-components": "^5.1.1"
  },
  "devDependencies": {
    "@percy/cypress": "^2.1.1",
    "axe-core": "^3.5.4",
    "babel-eslint": "^10.1.0",
    "chalk": "^4.0.0",
    "contentful-import": "^7.8.2",
    "cypress": "^4.7.0",
    "cypress-axe": "^0.8.1",
    "dotenv": "^8.2.0",
    "envfile": "^6.2.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-cypress": "^2.11.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.20.0",
    "inquirer": "^7.0.4",
    "prettier": "^2.0.5",
    "start-server-and-test": "^1.11.0"
  }
}
